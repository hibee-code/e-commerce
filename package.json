{
  "name": "lawma_app_backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "type": "commonjs",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "orm:mig:gen": "sh -c 'npm run typeorm -- migration:generate src/config/database/migrations/$0 -d ./src/config/database/connections/default.ts'",
    "orm:mig:run": "sh -c 'npm run typeorm -- migration:run -d ./src/config/database/connections/default.ts'",
    "orm:mig:create": "sh -c 'npm run typeorm -- migration:create src/config/database/migrations/$0'",
    "orm:seed:create": "sh -c 'npm run typeorm -- migration:create src/config/database/seeds/$0'",
    "create:db:entity": "sh -c 'typeorm-ts-node-esm entity:create src/$0'",
    "create:db:subscriber": "sh -c 'typeorm-ts-node-esm subscriber:create src/$0'",
    "create:db:migrations": "sh -c 'typeorm-ts-node-esm migration:create src/config/database/migrations/$0'",
    "revert:db:migrations": "sh -c 'typeorm-ts-node-esm migration:revert -d src/config/database/connections/default.ts'",
    "show:db:migrations": "sh -c 'typeorm-ts-node-esm migration:show -d src/config/database/connections/default.ts'",
    "sync:db:schema": "sh -c 'typeorm-ts-node-esm schema:sync -d src/config/database/connections/default.ts'",
    "log:db:schema": "sh -c 'typeorm-ts-node-esm schema:log -d src/config/database/connections/default.ts'",
    "drop:db:schema": "typeorm-ts-node-esm schema:drop -d src/config/database/connections/default.ts",
    "query:db": "sh -c 'typeorm-ts-node-esm query $0 -d src/config/database/connections/default.ts'",
    "clear:db:migrations:cache": "sh -c 'typeorm-ts-node-esm cache:clear -d src/config/database/connections/default.ts'"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/typeorm": "^10.0.1",
    "@sendgrid/mail": "^7.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/lodash": "^4.14.199",
    "@types/pg": "^8.10.3",
    "@types/until-promise": "^0.3.1",
    "axios": "^1.6.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "find-package-json": "^1.2.0",
    "global": "^4.4.0",
    "hashids": "^2.3.0",
    "helmet": "^7.0.0",
    "json-loader": "^0.5.7",
    "mathjs": "^12.1.0",
    "nestjs-pino": "^3.5.0",
    "nvm": "^0.0.4",
    "pg": "^8.11.3",
    "pino": "^8.15.6",
    "pino-http": "^8.5.0",
    "pino-pretty": "^10.2.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sib-api-v3-sdk": "^8.5.0",
    "typeorm": "^0.3.20",
    "until-promise": "^0.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.7.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  }
}
